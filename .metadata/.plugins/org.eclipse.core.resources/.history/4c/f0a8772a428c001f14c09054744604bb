package org.riss.bizconnect.hr.document.model.service;

import java.util.List;

import org.riss.bizconnect.hr.document.model.dto.Contract;

public interface ContractService {
	   
	// 증명서 목록 조회
    List<Contract> selectContracts();

    // 증명서 상세 조회
    Contract selectContractById(String contractId);

    // 증명서 추가
    int insertContract(Contract contract);

    // 증명서 수정
    int updateContract(Contract contract);

    // 증명서 삭제
	int deleteContract(String contractId);

    
}
