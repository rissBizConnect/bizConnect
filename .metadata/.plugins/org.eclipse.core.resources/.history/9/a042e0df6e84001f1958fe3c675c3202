package org.riss.bizconnect.hr.retire.model.dto;

public class Retire implements java.io.Serializable {

	private static final long serialVersionUID = 1L;
	
	 private int retire_no; //퇴직자 번호
	    private String user_entry_date; //입사일
	    private String ret_date;	//퇴사일
	    private String ret_type;	//비고
	    
	    public Retire() {
			super();
		}

		public Retire(int retire_no, String user_entry_date, String ret_date, String ret_type) {
			super();
			this.retire_no = retire_no;
			this.user_entry_date = user_entry_date;
			this.ret_date = ret_date;
			this.ret_type = ret_type;
		}

		public int getRetire_no() {
			return retire_no;
		}

		public void setRetire_no(int retire_no) {
			this.retire_no = retire_no;
		}

		public String getUser_entry_date() {
			return user_entry_date;
		}

		public void setUser_entry_date(String user_entry_date) {
			this.user_entry_date = user_entry_date;
		}

		public String getRet_date() {
			return ret_date;
		}

		public void setRet_date(String ret_date) {
			this.ret_date = ret_date;
		}

		public String getRet_type() {
			return ret_type;
		}

		public void setRet_type(String ret_type) {
			this.ret_type = ret_type;
		}

		public static long getSerialversionuid() {
			return serialVersionUID;
		}

		@Override
		public String toString() {
			return "Retire [retire_no=" + retire_no + ", user_entry_date=" + user_entry_date + ", ret_date=" + ret_date
					+ ", ret_type=" + ret_type + "]";
		}
	    
	    

}
