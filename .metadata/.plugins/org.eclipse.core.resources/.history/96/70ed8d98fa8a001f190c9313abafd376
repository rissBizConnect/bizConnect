package org.riss.bizconnect.hr.document.model.service;

import java.util.ArrayList;

import org.riss.bizconnect.hr.document.model.dto.Certificate;

public interface CertificateService {
	   
	int insertCertificate(Certificate certificate); // 증명서 추가
	ArrayList<Certificate> selectCertificates(Certificate certificate); // 증명서 목록 조회
	int updateCertificate(Certificate certificate); // 증명서 수정
	int deleteCertificate(Certificate certificate); // 증명서 삭제
}
