package org.riss.bizconnect.hr.document.model.dto;

import java.io.Serializable;
import java.sql.Date;

public class Certificate implements Serializable {
	
	private static final long serialVersionUID = 8621293571373691741L;
	
	private String documentId;    // 계약서/증명서 ID
    private Date dmDate;        // 등록 날짜
    private String dmFile;        // 파일 경로
    private String dmTitle;       // 서류명
    private String dmDescription; // 기타 설명
    private String gid;           // 로그인 아이디
    private String comCode;       // 회사 코드
	
	public Certificate() {
		super();
		// TODO Auto-generated constructor stub
	}

	public String getDocumentId() {
		return documentId;
	}

	public void setDocumentId(String documentId) {
		this.documentId = documentId;
	}

	public Date getDmDate() {
		return dmDate;
	}

	public void setDmDate(Date dmDate) {
		this.dmDate = dmDate;
	}

	public String getDmFile() {
		return dmFile;
	}

	public void setDmFile(String dmFile) {
		this.dmFile = dmFile;
	}

	public String getDmTitle() {
		return dmTitle;
	}

	public void setDmTitle(String dmTitle) {
		this.dmTitle = dmTitle;
	}

	public String getDmDescription() {
		return dmDescription;
	}

	public void setDmDescription(String dmDescription) {
		this.dmDescription = dmDescription;
	}

	public String getGid() {
		return gid;
	}

	public void setGid(String gid) {
		this.gid = gid;
	}

	public String getComCode() {
		return comCode;
	}

	public void setComCode(String comCode) {
		this.comCode = comCode;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	public Certificate(String documentId, Date dmDate, String dmFile, String dmTitle, String dmDescription, String gid,
			String comCode) {
		super();
		this.documentId = documentId;
		this.dmDate = dmDate;
		this.dmFile = dmFile;
		this.dmTitle = dmTitle;
		this.dmDescription = dmDescription;
		this.gid = gid;
		this.comCode = comCode;
	}
	
	@Override
	public String toString() {
		return "Document [documentId=" + documentId + ", dmDate=" + dmDate + ", dmFile=" + dmFile + ", dmTitle="
				+ dmTitle + ", dmDescription=" + dmDescription + ", gid=" + gid + ", comCode=" + comCode + "]";
	}

	
}
