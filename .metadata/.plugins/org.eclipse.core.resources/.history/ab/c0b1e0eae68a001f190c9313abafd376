package org.riss.bizconnect.hr.document.model.service;

public interface DocumentService {
	void addContract(FileDto fileDto); // 계약서 추가
    void addCertificate(FileDto fileDto); // 증명서 추가
    List<FileDto> getContractList(); // 계약서 목록 조회
    List<FileDto> getCertificateList(); // 증명서 목록 조회
    void updateContract(FileDto fileDto); // 계약서 수정
    void updateCertificate(FileDto fileDto); // 증명서 수정
    void deleteContract(String contractId); // 계약서 삭제
    void deleteCertificate(String certificateId); // 증명서 삭제
	
}
