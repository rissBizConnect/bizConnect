package org.riss.bizconnect.hr.retire.model.service;

import java.util.ArrayList;
import java.util.List;

import org.riss.bizconnect.hr.retire.model.dto.Retire;

public interface RetireService {

		List<Retire> getRetireeList();
	    
	    // 퇴직자 추가
	    int insertRetiree(Retire retiree);

	    // 퇴직자 수정
	    int updateRetiree(Retire retiree);

	    // 퇴직자 삭제
	    int deleteRetiree(int retireId);

}
